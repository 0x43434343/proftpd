@SET_MAKE@

top_builddir=@top_builddir@
top_srcdir=@top_srcdir@
srcdir=@srcdir@
VPATH=@srcdir@
DESTDIR=

include ../Make.rules

.PHONY: ""

all:
	@echo "Running make from top level directory."
	cd .. && $(MAKE) all

Makefile: Makefile.in ../config.status
	cd ../ && ./config.status

install-utils:
	$(INSTALL) ftpasswd	$(DESTDIR)$(bindir)/ftpasswd
	$(INSTALL) ftpmail	$(DESTDIR)$(bindir)/ftpmail
	$(INSTALL) ftpquota	$(DESTDIR)$(bindir)/ftpquota
