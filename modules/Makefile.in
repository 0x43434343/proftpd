CC=@CC@

top_srcdir=@top_srcdir@
srcdir=@srcdir@
VPATH=@srcdir@

@SET_MAKE@

include ../Make.rules
include ../Make.modules

.c.o:
	$(CC) $(CPPFLAGS) $(CFLAGS) -c $<

all:
	@echo "Running make from top level directory."
	cd ..; $(MAKE) all

module_glue.c: $(MODULES)
	srcdir=$(srcdir) $(srcdir)/glue.sh $(MODULES)

module_glue.o: module_glue.c
	$(CC) $(CPPFLAGS) $(CFLAGS) -c $<

modules: $(MODULES) module_glue.o

clean:
	rm -f *.o core *.save module_glue.c *~

depend:
	rm -f module_glue.c
	makedepend $(CPPFLAGS) -Y *.c 2>/dev/null
	makedepend $(CPPFLAGS) -Y -fMakefile.in *.c 2>/dev/null

